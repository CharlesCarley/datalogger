<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.charlescarley.datalogger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".ApplicationMain"
        android:theme="@style/BaseTheme">

        <activity android:name=".ActivitySet">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityEditSet" />
        <activity android:name=".ActivitySeries" />
        <activity android:name=".ActivityColorChooser" />
        <activity android:name=".ActivityBase" />
        <activity android:name=".ActivityEditSeries" />
        <activity android:name=".ActivityCollection" />
        <activity android:name=".ActivityEditCollection" />
        <activity android:name=".ActivityDisplayCollectionElement" />
    </application>

</manifest>